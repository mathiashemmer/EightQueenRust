{
	"version": "2.0.0",
	"tasks": [
        {
            "label": "run",
            "type": "shell",
            "problemMatcher": [
                "$rustc"
            ],
			"group": {
				"kind": "build",
				"isDefault": true
			},
            "command": "cargo",
            "args": [
                "run",
                "--release"
            ]
        }
    ]
}